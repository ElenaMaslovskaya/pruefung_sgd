<%- include("kopf") %>
    <h1>Neuen Kontakt erstellen</h1>
    <form action="/neu" method="post">
        <label for="firstname">Vorname:</label><br>
        <input type="text" id="firstname" name="firstname" required><br><br>
    
        <label for="lastname">Nachname:</label><br>
        <input type="text" id="lastname" name="lastname" required><br><br>
    
        <label for="street">Straße:</label><br>
        <input type="text" id="street" name="street" required><br><br>
    
        <label for="houseNumber">Hausnummer:</label><br>
        <input type="text" id="houseNumber" name="houseNumber" required><br><br>
    
        <label for="postalCode">Postleitzahl:</label><br>
        <input type="text" id="postalCode" name="postalCode" required><br><br>
    
        <label for="city">Stadt:</label><br>
        <input type="text" id="city" name="city" required><br><br>
    
        <button type="submit">Kontakt hinzufügen</button>
    </form>
<%- include("fuss") %>